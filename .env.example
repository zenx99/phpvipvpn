# Environment Configuration for VIP VPN HTTPS Server

# Server Ports
HTTPS_PORT=443
HTTP_PORT=80

# Enable HTTP to HTTPS redirect
ENABLE_HTTP_REDIRECT=true

# Environment (development/production)
NODE_ENV=production

# SSL Certificate paths (update these if different)
SSL_CERT_PATH=/etc/letsencrypt/live/scriptbotonline.vipv2boxth.xyz/fullchain.pem
SSL_KEY_PATH=/etc/letsencrypt/live/scriptbotonline.vipv2boxth.xyz/privkey.pem

# Domain
DOMAIN=scriptbotonline.vipv2boxth.xyz
